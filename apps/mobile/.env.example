# CareBow Environment Variables
# Copy this file to .env and fill in your values

# App Configuration
APP_ENV=development
APP_NAME=CareBow

# API Configuration
API_BASE_URL=https://api.carebow.com/v1
API_TIMEOUT=30000

# Ask CareBow AI Configuration
ASK_CAREBOW_API_URL=https://api.carebow.com/v1/ask-carebow
ASK_CAREBOW_API_KEY=

# Analytics (optional)
ANALYTICS_KEY=

# Sentry Error Tracking (optional)
SENTRY_DSN=

# Feature Flags
FEATURE_HEALTH_MEMORY=true
FEATURE_IMAGE_UPLOAD=true
FEATURE_VOICE_INPUT=true
